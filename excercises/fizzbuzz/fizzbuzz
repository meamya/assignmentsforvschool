// on multiples of three, console.log ("fizz")
// on multiples of five, console. log ("buzz")
//if both, console.log("fizz buzz")
//1 thru 100,
for (var i = 1; i < 101; i++) {
    console.log(i)
    if (i % 3 == 0) {
        console.log("fizz");
    }
    if (i % 5 == 0) {
        console.log("buzz");
        if (i % 3 !== 0 && i % 5 !== 0) {
            console.log(i)
        }
        console.log("FizzBuzz!")